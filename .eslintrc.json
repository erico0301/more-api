{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":0,
        "no-console":0,
        "indent":["error", 4],
        "no-underscore-dangle": 0,
        "max-len": 0,
        "object-curly-newline": 0,
        "camelcase": 0
    }
}
